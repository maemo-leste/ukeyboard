header {
name	"Arabic (Lebanon)"
lang	"ar_LB"
wc	"ar_LB"

size	0	{ width 35 height 35 textpos 26 left 0 top 0 }
size	1	{ width 35 height 35 textpos 26 left 0 top 0 }
size	2	{ width 66 height 55 textpos 41 left 0 top 0 }
size	3	{ width 131 height 55 textpos 41 left 0 top 0 }
}

kbd_normal {
